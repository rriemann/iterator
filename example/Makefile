####### Compiler, tools and options

CC            = gcc
CXX           = g++
CFLAGS        = -m64 -pipe -O2 -Wall -W -g
CXXFLAGS      = -m64 -pipe -std=c++11 -O2 -Wall -W -g
LINK          = g++
LFLAGS        = -m64 -std=c++11 -Wl,-O1
DEL_FILE      = rm -f
SYMLINK       = ln -f -s
DEL_DIR       = rmdir
MOVE          = mv -f
CHK_DIR_EXISTS= test -d
MKDIR         = mkdir -p

####### Output directory

SOURCES       = node_iterator1.cpp 
OBJECTS       = node_iterator1.o

TARGET        = example

first: all
####### Implicit rules

.SUFFIXES: .o .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o "$@" "$<"

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(OBJECTS)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJCOMP) $(LIBS)

dist: 
	@$(CHK_DIR_EXISTS) .tmp/example1.0.0 || $(MKDIR) .tmp/example1.0.0 
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/example1.0.0/ && (cd `dirname .tmp/example1.0.0` && $(TAR) example1.0.0.tar example1.0.0 && $(COMPRESS) example1.0.0.tar) && $(MOVE) `dirname .tmp/example1.0.0`/example1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/example1.0.0


clean:compiler_clean 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) 
	-$(DEL_FILE) Makefile


check: first

compiler_clean: 

####### Compile

node_iterator1.o: node_iterator1.cpp node_iterator1.hpp \
		node.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o node_iterator1.o node_iterator1.cpp

####### Install

install:   FORCE

uninstall:   FORCE

FORCE:

